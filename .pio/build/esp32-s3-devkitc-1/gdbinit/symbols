# Load esp32s3 ROM ELF symbols
define target hookpost-remote
set confirm off
  # if $_streq((char *) 0x3ff194ad, "Mar  1 2021")
  if (*(int*) 0x3ff194ad) == 0x2072614d && (*(int*) 0x3ff194b1) == 0x32203120 && (*(int*) 0x3ff194b5) == 0x313230
    add-symbol-file C:/Users/heged/.platformio/packages/tool-esp-rom-elfsesp32s3_rev0_rom.elf
  else
    echo Warning: Unknown esp32s3 ROM revision.\n
  end
set confirm on
end


# Load bootloader symbols
set confirm off
    add-symbol-file A:/workspace/projects/WRO2026/.pio/build/esp32-s3-devkitc-1/bootloader/bootloader.elf
set confirm on

# Load application symbols
file A:/workspace/projects/WRO2026/.pio/build/esp32-s3-devkitc-1/ESP32S3-Template.elf
